{
  "name": "fyrox-vsc-extension",
	"displayName": "Fyrox Extension for Visual Studio Code",
	"description": "an extension for Visual Studio Code that includes snippets and stuff for the fyrox game engine",
	"version": "0.0.1",
	"publisher": "DuckEater54",
    "repository": {
        "type": "git",
        "url": "https://github.com/DuckEater54/Fyrox-VSCODE.git"
    },
	"keywords": [
		"game engine",
		"fyrox",
		"ge",
		"gamedev",
		"rust"
	],
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
        "onCommand:fyrox-ge-extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fyrox-ge-extension.helloWorld",
				"title": "Hello World"
			}
		],
		"snippets": [
			{
				"language": "rust",
				"path": "./snippets/fyrox.code-snippets"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
